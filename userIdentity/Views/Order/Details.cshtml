@model IEnumerable<OrderItem>



  

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.courses.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.courses.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.courses.ImageUrl)
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.courses.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.courses.Price)
            </td>
            <td>
                <img height="140px" width="140px" src="~/images/courses/@Html.DisplayFor(modelItem => item.courses.ImageUrl)">
            </td>
            
        </tr>
}
    </tbody>
</table>
